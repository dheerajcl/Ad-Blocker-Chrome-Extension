{
    "manifest_version": 3,
    "name": "Ad Blocker",
    "version": "1.0.0",
    "description": "An ad blocker extension that provides a seamless and distraction-free browsing experience.",
    "author": "Dheeraj C L",
    "permissions": [
        "declarativeNetRequest"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
